    "script-ext-html-webpack-plugin": "1.3.2",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.1.5",
    "string-replace-loader": "1.0.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "1.1.2",
    "ts-node": "^1.7.0",
    "tslint": "^3.9.0",
    "tslint-loader": "^2.1.3",
    "typedoc": "0.5.0",
    "typescript": "2.0.6",
    "url-loader": "^0.5.7",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "^1.6.1",
